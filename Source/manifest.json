{
  "manifest_version": 2,

  "name": "Beat Xichgan",
  "description": "This extension replaces every M with X for Beat Xichgan week",
  "version": "1.0.1",
  "content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["beatxichigan.js"],
			"run_at": "document_end"
		}
	],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
